<div class="row">
  <h1>Login</h1>
</div>
<%= form_with url: "/sessions" do |form| %>
    <div class="row">
    <h5>
      <%= form.label "email" %>
      <%= form.text_field "email" %>
    </h5>
    </div>
  <div class="row">
    <h5>
      <%= form.label "password" %>
      <%= form.password_field "password" %>
    </h5>
  </div>
  <div class="row">
    <h4>
    <%= form.submit "Login" , class: "btn btn-primary"%>
   </h4>
  </div>
<% end %>